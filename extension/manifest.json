{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0",
  "description": "Tracks active browsing time",
  "permissions": ["tabs", "storage", "alarms", "identity", "identity.email"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
